{
  "mainClass": "org.apache.spark.repl.Main?",
  "repositories": [
    "central"
  ],
  "scalaVersion": "2.12+",
  "dependencies": [
    "org.apache.spark::spark-repl:latest.stable",
    "org.slf4j:slf4j-nop:1.7.28"
  ],
  "exclusions": [
    "org.slf4j:slf4j-log4j12"
  ],
  "properties": {
    "scala.usejavacp": "true",
    "spark.master?": "local[*]",
    "spark.master": "local[*]"
  }
}
